{
  "name": "M75Dash3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build": "cd android && ./gradlew bundleRelease",
    "build_new": "npx react-native build-android --mode=release",
    "clean": "cd android && ./gradlew clean",
    "apk_new": "npm run android -- --mode='release'",
    "apk": "cd android && ./gradlew assembleRelease",
    "device": "react-native run-android --deviceId 68efca1a",
    "cc": "react-native start --reset-cache",
    "cc2": "npm start -- --reset-cache",
    "log-ios": "npx react-native log-ios",
    "log-android": "npx react-native log-android",
    "loadAPK": "npx react-native run-android --variant=release",
    "loadapk": "npx react-native run-android --mode=release",
    "createAPK": "npm run android -- --mode=\"release\"",
    "del": "cd android && rm -rf build && cd app && rm -rf build",
    "delW": "cd android && rmdir build /s /q && cd app && rmdir build /s /q",
    "new": "yarn clean && yarn del && yarn apk"
  },
